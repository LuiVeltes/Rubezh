<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:AutomationModule.Views"
					xmlns:ViewModels="clr-namespace:AutomationModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:FiltersViewModel}">
		<Views:FiltersView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:FiltersMenuViewModel}">
		<Views:FiltersMenuView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:FilterDetailsViewModel}">
		<Views:FilterDetailsView />
	</DataTemplate>
</ResourceDictionary>
