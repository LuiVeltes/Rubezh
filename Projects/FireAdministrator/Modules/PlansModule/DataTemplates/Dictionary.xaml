<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:PlansModule.Views"
                    xmlns:ViewModels="clr-namespace:PlansModule.ViewModels">
    
    <DataTemplate DataType="{x:Type ViewModels:PlansViewModel}">
        <Views:PlansView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type ViewModels:PlansMenuViewModel}">
        <Views:PlansMenuView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type ViewModels:PlanDetailsViewModel}">
        <Views:PlanDetailsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:DevicesViewModel}">
        <Views:DevicesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:SubPlanDetailsViewModel}">
        <Views:SubPlanDetailsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:RectanglePropertiesViewModel}">
        <Views:RectanglePropertiesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:EllipsePropertiesViewModel}">
        <Views:EllipsePropertiesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:PolygonPropertiesViewModel}">
        <Views:PolygonPropertiesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:TextBlockPropertiesViewModel}">
        <Views:TextBlockPropertiesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ZonePropertiesViewModel}">
        <Views:ZonePropertiesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:DevicePropertiesViewModel}">
        <Views:DevicePropertiesView />
    </DataTemplate>

</ResourceDictionary>