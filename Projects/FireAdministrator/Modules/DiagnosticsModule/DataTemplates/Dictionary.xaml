<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:DiagnosticsModule.Views"
                    xmlns:ViewModels="clr-namespace:DiagnosticsModule.ViewModels">

    <DataTemplate DataType="{x:Type ViewModels:DiagnosticsViewModel}">
        <Views:DiagnosticsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:DevicesTreeViewModel}">
        <Views:DevicesTreeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:JournalTestViewModel}">
        <Views:JournalTestView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:DeviceIconTestViewModel}">
        <Views:DeviceIconTestView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ZoneTestViewModel}">
        <Views:ZoneTestView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:TreeViewTestViewModel}">
        <Views:TreeViewTestView />
    </DataTemplate>
</ResourceDictionary>