<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:SecurityModule.Views"
                    xmlns:ViewModels="clr-namespace:SecurityModule.ViewModels">

    <DataTemplate DataType="{x:Type ViewModels:SecurityViewModel}">
        <Views:SecurityView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:NewUserViewModel}">
        <Views:NewUserView />
    </DataTemplate>
</ResourceDictionary>