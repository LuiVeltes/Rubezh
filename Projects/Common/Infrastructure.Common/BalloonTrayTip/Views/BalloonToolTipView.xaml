<UserControl x:Class="Infrastructure.Common.BalloonTrayTip.Views.BalloonToolTipView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Loaded="UserControl_Loaded"
             xmlns:tb="http://www.hardcodet.net/taskbar"
             Visibility="Hidden" Name="control">
    <Grid>
        <tb:TaskbarIcon
            x:Name="taskbarIcon"
            Visibility="Hidden"
            Unloaded="taskbarIcon_Unloaded"/>
    </Grid>
</UserControl>
