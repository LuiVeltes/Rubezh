<UserControl x:Class="Infrastructure.Common.Video.RVI_VSS.CellPlayerWrap"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:winFormsPlayer="clr-namespace:WinFormsPlayer;assembly=WinFormsPlayer"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="SlateGray">
        <WindowsFormsHost>
            <winFormsPlayer:WinFormsPlayer x:Name="FormsPlayer"></winFormsPlayer:WinFormsPlayer>
        </WindowsFormsHost>
    </Grid>
</UserControl>
