<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="HighlightDarkColor">#FFC5C5BF</Color>
    <Color x:Key="HighlightLightColor">#FFC5C5BF</Color>
    <Color x:Key="AccentColor">#FFC5C5BF</Color>
    <!--60%-->
    <Color x:Key="AccentColor2">#FFC5C5BF</Color>
    <!--40%-->
    <Color x:Key="AccentColor3">#FFC5C5BF</Color>
    <!--20%-->
    <Color x:Key="AccentColor4">#FFC5C5BF</Color>
    <LinearGradientBrush x:Key="BrandingBrush" EndPoint="0.001,0.5" StartPoint="1.002,0.5">
        <GradientStop Color="{DynamicResource HighlightDarkColor}" Offset="0"/>
        <GradientStop Color="#FFC5C5BF" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="AccentColorBrush"
                     Color="{DynamicResource AccentColor}" />
</ResourceDictionary>