<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<LinearGradientBrush x:Key="WindowBlacHeaderkBackgroundBrush" StartPoint="0, 0" EndPoint="0, 1" Opacity="0">
            <GradientStop Color="Black" Offset="0"/>
            <GradientStop Color="Black" Offset=".2"/>
            <GradientStop Color="#4172A3" Offset=".4"/>
    </LinearGradientBrush>
	
    <LinearGradientBrush x:Key="BaseWindowBackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
	<GradientStop Color="#266199" Offset="0"/>
	<GradientStop Color="#266199" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DialogHeaderBrush" StartPoint="0,0" EndPoint="0,1">
            <GradientStop Color="LightBlue" Offset="0"/>
            <GradientStop Color="LightSteelBlue" Offset=".02"/>
            <GradientStop Color="SteelBlue" Offset=".4"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="BackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
    <GradientStop Color="#698EBE" Offset="0.2" />
    <GradientStop Color="#5980B2" Offset="0.4" />
    <GradientStop Color="#5980B2" Offset="0.6" />
    <GradientStop Color="#5D86BD" Offset="0.85" />
    <GradientStop Color="#5980B2" Offset="0.95" />
    <GradientStop Color="#698EBE" Offset="1" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HighlightedBackgoundBrush" StartPoint="0, 0" EndPoint="0,1">
        <GradientStop Color="#F2B352" Offset="0" />
        <GradientStop Color="#DF8E2A" Offset="1" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HighlightedBackgoundBrush2" StartPoint="0, 0" EndPoint="0,1">
        <GradientStop Color="#F2B352" Offset="0" />
        <GradientStop Color="#DF8E2A" Offset="1" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PressedBackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
        <GradientStop Color="#DEA34B" Offset="0" />
        <GradientStop Color="#C98226" Offset="1" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="AlternativeBackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
        <GradientStop Color="#55A3FF" Offset="0" />
        <GradientStop Color="#55A3FF" Offset="1" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="WindowBorderBrush" Color="Black" />
    <SolidColorBrush x:Key="DisabledBorderBrush" Color="DarkGray" />
    <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="LightGray" />
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="Gray" />
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="White" />

</ResourceDictionary>