    4 - Обход устройств
    5 - Неизвестно
    6 - Норма -->

<!-- Коды состояний панели    
     0 - Внимание в системе
     1 - Внимание в зоне
     2 - Тест Лазер
     3 - Тест "Кнопка"
     4 - Пожар
     5 - Неисправность
     6 - Потеря связи с устройством
     7 - Пуск АСПТ в ручном режиме
     8 - Ошибка конфигурирования БД
     9 - Блокировка
     10 - Запыленность: предупреждение
     11 - Запыленность: критический уровень
     12 - Ошибка обновления ПО

     32 - Потеря связи с прибором

-->


  <deviceStates>
    <deviceState class="2" name="Потеря связи" ID="ConnectionLost">
      <enter event="$09"/>
      <leave event="$0A" />
      <leave panelState="5"/>
    </deviceState>

    <deviceState class="0" name="Пожарная тревога" ID="FireAlarm">
      <enter event="$01"/>
      <leave panelState="4"/>
    </deviceState>

    <deviceState class="6" name="Тест 'Кнопка'" ID="TestButton">
      <enter event="$02"/>
      <leave panelState="3"/>
    </deviceState>

    <deviceState class="6" name="'Тест Лазер'" ID="TestLaser">
      <enter event="$03"/>
      <leave panelState="2"/>
    </deviceState>

    <deviceState class="1" name="Внимание" ID="Warning">
      <enter event="$0E"/>
      <leave panelState="0"/>
    </deviceState>

    <deviceState class="2" name="Неисправность" ID="Malfunction">
      <enter event="$23"/>
      <leave event="$24"/>
      <leave panelState="5"/>
    </deviceState>

    <deviceState class="3" name="Предварительный уровень запыленности" ID="LowDustiness">
      <enter event="$25"/>
      <leave event="$26"/>
      <leave panelState="10"/>
    </deviceState>

    <deviceState class="3" name="Критический уровень запыленности" ID="HighDustiness">
      <enter event="$27"/>
      <leave event="$28"/>
      <leave panelState="11"/>
    </deviceState>

<!--    <deviceState class="2" name="Потеря связи с прибором" ID="ParentConnectionLost"/> -->

  </deviceStates>

