<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:SKDModule.Views"
                    xmlns:ViewModels="clr-namespace:SKDModule.ViewModels">

    <DataTemplate DataType="{x:Type ViewModels:AdditionalColumnTypesViewModel}">
        <Views:AdditionalColumnTypesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:AdditionalColumnTypeDetailsViewModel}">
        <Views:AdditionalColumnTypeDetailsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:AdditionalColumnTypeFilterViewModel}">
        <Views:AdditionalColumnTypeFilterView />
    </DataTemplate>

</ResourceDictionary>