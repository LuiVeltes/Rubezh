<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:SKDModule.Views"
					xmlns:ViewModels="clr-namespace:SKDModule.ViewModels">

    <DataTemplate DataType="{x:Type ViewModels:EmployeesViewModel}">
        <Views:EmployeesView />
	</DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:EmployeeFilterViewModel}">
        <Views:EmployeeFilterView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:EmployeeDetailsViewModel}">
        <Views:EmployeeDetailsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:EmployeeCardDetailsViewModel}">
		<Views:EmployeeCardDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:CardRemovalReasonViewModel}">
		<Views:CardRemovalReasonView />
	</DataTemplate>
	
</ResourceDictionary>