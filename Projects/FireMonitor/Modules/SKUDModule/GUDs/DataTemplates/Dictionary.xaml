<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:SKDModule.Views"
					xmlns:ViewModels="clr-namespace:SKDModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:GUDsViewModel}">
		<Views:GUDsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:GUDDetailsViewModel}">
		<Views:GUDDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:GUDFilterViewModel}">
		<Views:GUDFilterView />
	</DataTemplate>

</ResourceDictionary>