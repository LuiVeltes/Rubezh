<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:PlansModule.Views"
                    xmlns:ViewModels="clr-namespace:PlansModule.ViewModels">
    
    <DataTemplate DataType="{x:Type ViewModels:PlansViewModel}">
        <Views:PlansView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:VideoViewModel}">
        <Views:VideoView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:DoorViewModel}">
        <Views:DoorView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:PhoneViewModel}">
        <Views:PhoneView />
    </DataTemplate>

</ResourceDictionary>