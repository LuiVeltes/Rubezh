<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:JournalModule.Views"
                    xmlns:ViewModels="clr-namespace:JournalModule.ViewModels">

    <DataTemplate DataType="{x:Type ViewModels:JournalsViewModel}">
        <Views:JournalsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ArchiveViewModel}">
        <Views:ArchiveView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ArchiveFilterViewModel}">
        <Views:ArchiveFilterView />
    </DataTemplate>

</ResourceDictionary>