        <GridSplitter Grid.Row="0" Grid.Column="1" Grid.RowSpan="11" Width="5" Background="Transparent" VerticalAlignment="Stretch" HorizontalAlignment="Center" />
        <GridSplitter Grid.Row="0" Grid.Column="3" Grid.RowSpan="11" Width="5" Background="Transparent" VerticalAlignment="Stretch" HorizontalAlignment="Center" />
        <GridSplitter Grid.Row="0" Grid.Column="5" Grid.RowSpan="11" Width="5" Background="Transparent" VerticalAlignment="Stretch" HorizontalAlignment="Center" />
        <GridSplitter Grid.Row="0" Grid.Column="7" Grid.RowSpan="11" Width="5" Background="Transparent" VerticalAlignment="Stretch" HorizontalAlignment="Center" />
