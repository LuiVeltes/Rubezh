        <views:LayoutPartCameraView x:Name="_grid05" Grid.Row ="0" Grid.Column="10" Background="{DynamicResource BackgroundBrush}" />

        <views:LayoutPartCameraView x:Name="_grid10" Grid.Row ="2" Grid.Column="0" Background="{DynamicResource BackgroundBrush}" />
