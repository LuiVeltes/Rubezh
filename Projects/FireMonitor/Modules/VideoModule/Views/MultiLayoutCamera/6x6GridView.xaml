<UserControl x:Class="VideoModule.Views._6X6GridView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:views="clr-namespace:VideoModule.Views">

        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition MinHeight="10" />
                <RowDefinition Height="Auto" MinHeight="5" />
                <RowDefinition MinHeight="10" />
                <RowDefinition Height="Auto" MinHeight="5" />
                <RowDefinition MinHeight="10" />
                <RowDefinition Height="Auto" MinHeight="5" />
                <RowDefinition MinHeight="10" />
                <RowDefinition Height="Auto" MinHeight="5" />
                <RowDefinition MinHeight="10" />
                <RowDefinition Height="Auto" MinHeight="5" />
                <RowDefinition MinHeight="10" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition MinWidth="10" />
                <ColumnDefinition Width="auto" MinWidth="5" />
                <ColumnDefinition MinWidth="10" />
                <ColumnDefinition Width="auto" MinWidth="5" />
                <ColumnDefinition MinWidth="10" />
                <ColumnDefinition Width="auto" MinWidth="5" />
                <ColumnDefinition MinWidth="10" />
                <ColumnDefinition Width="auto" MinWidth="5" />
                <ColumnDefinition MinWidth="10" />
                <ColumnDefinition Width="auto" MinWidth="5" />
                <ColumnDefinition MinWidth="10" />
            </Grid.ColumnDefinitions>
        <views:LayoutPartCameraView x:Name="_grid00" Grid.Row ="0" Grid.Column="0" Background="{DynamicResource BackgroundBrush}" />
        <views:LayoutPartCameraView x:Name="_grid01" Grid.Row ="0" Grid.Column="2" Background="{DynamicResource BackgroundBrush}" />
        <views:LayoutPartCameraView x:Name="_grid02" Grid.Row ="0" Grid.Column="4" Background="{DynamicResource BackgroundBrush}" />
        <views:LayoutPartCameraView x:Name="_grid03" Grid.Row ="0" Grid.Column="6" Background="{DynamicResource BackgroundBrush}" />
