<UserControl x:Class="DiagnosticsModule.Views.DiagnosticsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid Margin="10">
        <StackPanel>
            <Button Content="Имитатор" Command="{Binding ShowImitatorCommand}" Width="100" Margin="2" />
            <Button Content="Poll" Command="{Binding PollCommand}" Width="100" Margin="2" />
            <Button Content="StopPoll" Command="{Binding StopPollCommand}" Width="100" Margin="2" />
        </StackPanel>
    </Grid>
</UserControl>