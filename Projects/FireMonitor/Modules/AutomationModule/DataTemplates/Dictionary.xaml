<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:AutomationModule.Views"
					xmlns:ViewModels="clr-namespace:AutomationModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:ProceduresViewModel}">
		<Views:ProceduresView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:LayoutProcedurePartViewModel}">
		<Views:LayoutProcedurePartView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ProcedureArgumentsViewModel}">
		<Views:ProcedureArgumentsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ArgumentsViewModel}">
		<Views:ArgumentsView />
	</DataTemplate>

</ResourceDictionary>