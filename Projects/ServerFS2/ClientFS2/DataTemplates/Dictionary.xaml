<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:Views="clr-namespace:ClientFS2.Views" 
                    xmlns:ViewModels="clr-namespace:ClientFS2.ViewModels">
    <DataTemplate DataType="{x:Type ViewModels:JournalViewModel}">
        <Views:JournalView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:DevicesViewModel}">
        <Views:DevicesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ConfigurationDatabaseViewModel}">
        <Views:ConfigurationDatabaseView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:PropertiesViewModel}">
        <Views:PropertiesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ProgressViewModel}">
        <Views:ProgressView />
    </DataTemplate>
    
</ResourceDictionary>