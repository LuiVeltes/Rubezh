<Window x:Class="Diagnostics.DiagnosticsView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Менеджер логов" MaxHeight="300" MaxWidth="220" MinHeight="300" MinWidth="220">
    <StackPanel Background="LightBlue">
        <Button Background="Orange" Content="Собрать логи" Command="{Binding GetLogsCommand}" Height="30" Width="210" VerticalAlignment="Top" FontSize="15"/>
        <Button Background="Orange" Content="Удалить логи" Command="{Binding RemLogsCommand}" Height="30" Width="210" VerticalAlignment="Top" FontSize="15" />
    </StackPanel>
</Window>
